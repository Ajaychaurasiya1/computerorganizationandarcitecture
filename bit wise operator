#include<bits/stdc++.h>
using namespace std;

int main()
{
    int N;
    cin>>N;
    for(int i=1;i<=N;i++)
    {
        int A,B;
        cin>>A>>B;
        cout<<(A&B)<<endl;
        cout<<(A|B)<<endl;
        cout<<(A^B)<<endl;
        cout<<(~A)<<" "<<(~B)<<endl;
        cout<<(A<<1)<<" "<<(B<<1)<<endl;
        cout<<(A>>1)<<" "<<(B>>1)<<endl;

    }
    return 0;
}
